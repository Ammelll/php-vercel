{
  "version": 2,
  "functions": {
    "api/*.php": { "runtime": "now-php@1.0.0" }
  },
  "routes": [
    {
      "src": "/api/tutorial.php",
      "headers": { "Location": "/tutorial" },
      "status": 307
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/tutorial",
      "dest": "/api/tutorial.php"
    },
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    }
  ]
}